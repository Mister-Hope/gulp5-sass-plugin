{
  "name": "gulp5-sass-plugin",
  "version": "2.0.4",
  "description": "Gulp plugin for sass",
  "keywords": [
    "gulp-plugin",
    "sass",
    "gulp"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/gulp5-sass-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/gulp5-sass-plugin"
  },
  "license": "MIT",
  "author": "Mr.Hope",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "lint": "eslint --fix . && prettier --check --write .",
    "lint:check": "eslint . && prettier --check .",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up -r",
    "prepare": "husky",
    "prepublish": "pnpm clean && pnpm build",
    "test": "vitest --coverage"
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts}": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "picocolors": "^1.1.1",
    "plugin-error": "^2.0.1",
    "replace-ext": "^2.0.0",
    "sass": "^1.85.1",
    "source-map-js": "^1.2.1",
    "strip-ansi": "^7.1.0",
    "vinyl": "^3.0.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@codecov/rollup-plugin": "1.9.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@types/gulp": "4.0.17",
    "@types/gulp-postcss": "8.0.6",
    "@types/gulp-sourcemaps": "0.0.38",
    "@types/node": "22.13.11",
    "@types/vinyl": "2.0.12",
    "@vitest/coverage-istanbul": "3.0.9",
    "autoprefixer": "10.4.21",
    "del": "8.0.0",
    "eslint": "9.22.0",
    "eslint-config-mister-hope": "0.8.1",
    "gulp": "5.0.0",
    "gulp-postcss": "10.0.0",
    "gulp-sourcemaps": "3.0.0",
    "husky": "9.1.7",
    "nano-staged": "0.8.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "rollup": "4.36.0",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-esbuild": "6.2.1",
    "standard-version": "9.5.0",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "peerDependencies": {
    "gulp": "^5.0.0"
  },
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
