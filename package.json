{
  "name": "gulp5-sass-plugin",
  "version": "2.0.4",
  "description": "Gulp plugin for sass",
  "keywords": [
    "gulp",
    "gulp-plugin",
    "sass"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/gulp5-sass-plugin/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/gulp5-sass-plugin"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "dev": "tsdown --config-loader unrun -w",
    "lint": "oxlint --fix --type-aware --type-check --report-unused-disable-directives --deny-warnings && oxfmt",
    "lint:check": "oxlint --type-aware --type-check --report-unused-disable-directives --deny-warnings && oxfmt --check",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up -r",
    "prepare": "husky",
    "prepublish": "pnpm clean && pnpm build",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "picocolors": "^1.1.1",
    "plugin-error": "^2.0.1",
    "replace-ext": "^2.0.0",
    "sass": "^1.97.3",
    "source-map-js": "^1.2.1",
    "strip-ansi": "^7.1.2",
    "vinyl": "^3.0.1",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@codecov/rollup-plugin": "1.9.1",
    "@commitlint/cli": "20.4.0",
    "@commitlint/config-conventional": "20.4.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@types/gulp": "4.0.18",
    "@types/gulp-postcss": "8.0.6",
    "@types/gulp-sourcemaps": "0.0.38",
    "@types/node": "25.1.0",
    "@types/vinyl": "2.0.12",
    "@vitest/coverage-istanbul": "4.0.18",
    "autoprefixer": "10.4.24",
    "del": "8.0.1",
    "gulp": "5.0.1",
    "gulp-postcss": "10.0.0",
    "gulp-sourcemaps": "3.0.0",
    "husky": "9.1.7",
    "nano-staged": "0.9.0",
    "oxfmt": "0.27.0",
    "oxlint": "1.42.0",
    "oxlint-tsgolint": "0.11.4",
    "postcss": "8.5.6",
    "rimraf": "6.1.2",
    "standard-version": "9.5.0",
    "tsdown": "0.20.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "gulp": "^5.0.0"
  },
  "nano-staged": {
    "**/*": "oxfmt --no-error-on-unmatched-pattern",
    "*.{js,ts}": "oxlint --fix --type-aware --type-check --deny-warnings --report-unused-disable-directives"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@10.28.2"
}
